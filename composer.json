{
    "name": "lasalle/dw3",

    "description": "This is a dummy package for my students",
    "authors": [
        {
            "name": "Omar Faruk",
            "email": "omarfaruk.sust@gmail.com"
        }
    ],
    "require": {
        "monolog/monolog": "^3.2",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MyApp\\": "src/"
        }
    },
    "autoload-dev": {
